{
	"$schema": "https://json.schemastore.org/package.json",
	"name": "package-template",
	"private": "true",
	"version": "0.0.0",
	"description": "A template for packages",
	"author": {
		"name": "Amanda Guthrie",
		"url": "https://github.com/amandaguthrie"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/amandaguthrie/template-pnpm-typescript-monorepo.git",
		"directory": "packages/_template"
	},
	"license": "UNLICENSED",
	"type": "module",
	"main": "dist/index.cjs",
	"module": "dist/index.js",
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			},
			"required": {
				"types": "./dist/index.d.cts",
				"default": "./dist/index.cjs"
			}
		},
		"./package.json": "./package.json"
	},
	"files": [
		"dist"
	],
	"side-effects": false,
	"scripts": {
		"build": "tsdown",
		"build-fast": "tsdown --no-dts",
		"dev": "pnpm build-fast --watch",
		"test": "node --import tsx tests/test.mts"
	},
	"publishConfig": {
		"access": "restricted"
	}
}
